<mvc:View controllerName="int.ui5.template.controller.Home" xmlns="sap.m" xmlns:f="sap.f" xmlns:mvc="sap.ui.core.mvc" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:codeeditor="sap.ui.codeeditor" xmlns:uxap="sap.uxap"> 
	<uxap:ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" alwaysShowContentHeader="false" preserveHeaderStateOnScroll="false" headerContentPinnable="true" isChildPage="true" upperCaseAnchorBar="false" showFooter="true">                
		<uxap:headerTitle>                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
			<uxap:ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                         
				<uxap:heading>                                                                                                                                                                                                                                             
					<Title text="Search text in Custom BSP/UI5 Applications"></Title>                                                                                                                                                                                         
				</uxap:heading>                                                                                                                                                                                                                                            
			</uxap:ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                        
		</uxap:headerTitle>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
		<uxap:headerContent>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
			<HeaderContainer showDividers="false">                                                                                                                                                                                                                      
				<Input id="inputText" placeholder="Search" change="onChangeText" ariaLabelledBy="inputTextLabel" />                                                                                                                                                        
			</HeaderContainer>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
		</uxap:headerContent>                                                                                                                                                                                                                                        
		<uxap:ObjectPageSection title="Results" titleUppercase="false" subSections="{ path: 'application>/',  templateShareable:true }" class="sapUiSmallMarginTop">                                                                                                 
			<uxap:subSections>                                                                                                                                                                                                                                          
				<uxap:ObjectPageSubSection title="{application>Applname}">                                                                                                                                                                                                 
					<IconTabBar id="idIconTabBarInlineMode" headerMode="Inline" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding" items="{ path : 'application>Files/results' , templateShareable:true }">                                                 
						<items>                                                                                                                                                                                                                                                  
							<IconTabFilter text="{application>Pagename}" key="info" count="3">                                                                                                                                                                                      
								<codeeditor:CodeEditor type="json" value="{path : 'application>Lines' , formatter:'.formatLines' }" height="300px" />                                                                                                                                  
							</IconTabFilter>                                                                                                                                                                                                                                        
						</items>                                                                                                                                                                                                                                                 
					</IconTabBar>                                                                                                                                                                                                                                             
				</uxap:ObjectPageSubSection>                                                                                                                                                                                                                               
			</uxap:subSections>                                                                                                                                                                                                                                         
		</uxap:ObjectPageSection>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
	</uxap:ObjectPageLayout>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    